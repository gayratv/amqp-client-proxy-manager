    "esbuild": "^0.17.9",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",

    "ts-node": "^10.9.1",
